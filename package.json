{
  "name": "handleable",
  "version": "0.20.0",
  "description": "A library for synchronous and asynchronous error handling.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "prepublish": "npm run build",
    "test": "npm run build && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/handleablejs.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/handleablejs/issues"
  },
  "homepage": "https://github.com/xpepermint/handleablejs#readme",
  "keywords": [
    "handleable",
    "handling",
    "handle",
    "error",
    "errors",
    "ishandled",
    "check",
    "checking",
    "block"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.17.x",
    "typescript": "2.1.4"
  },
  "dependencies": {
    "lodash.merge": "4.6.0"
  }
}
